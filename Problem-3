import java.util.Scanner;

public class NumberSeriesGenerator {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter the value of a: ");
        int a = scanner.nextInt();

        for (int i = 1; i <= a; i += 2) {
            System.out.print(i);

            if (i + 2 <= a) {
                System.out.print(", ");
            }
        }

        scanner.close();
    }
}
